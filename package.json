    "eslint": "^8.18.0",
    "eslint-config-next": "^12.2.0",
    "eslint-config-sznm": "^1.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "next-sitemap": "^3.1.7",
    "prettier": "^2.7.1",
    "standard-version": "^9.5.0",
    "typescript": "^4.7.4"
