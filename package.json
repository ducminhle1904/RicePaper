    "@mantine/modals": "^5.2.4",
    "@mantine/next": "^5.2.4",
    "@mantine/notifications": "^5.2.4",
    "@mantine/nprogress": "^5.2.4",
    "@mantine/spotlight": "^5.2.4",
    "@tabler/icons": "^1.91.1",
    "animate.css": "^4.1.1",
    "embla-carousel-react": "^7.0.2",
    "framer-motion": "^6.3.16",
